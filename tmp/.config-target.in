	select USES_DEVICETREE
	select HAS_FPU
	select GPIO_SUPPORT
	select NAND_SUPPORT
	select PCI_SUPPORT
	select PCIE_SUPPORT
	select USES_INITRAMFS
	select RTC_SUPPORT
	select USES_SQUASHFS
	select HAS_TESTING_KERNEL
	select USB_SUPPORT
	select arm
	select HAS_DEVICES


